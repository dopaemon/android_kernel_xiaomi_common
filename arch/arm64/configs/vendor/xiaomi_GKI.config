# Device tree
CONFIG_BUILD_ARM64_DT_OVERLAY=y

# Fingerprint
CONFIG_FINGERPRINT_FPC_1540=y
CONFIG_FINGERPRINT_GOODIX_TEE=y
CONFIG_INPUT_FINGERPRINT=y

# Haptics
CONFIG_INPUT_AW862XX_HAPTIC=y
CONFIG_INPUT_AW86927_HAPTIC=y

# Hardware ID
CONFIG_MI_HARDWARE_ID=y

# IR
CONFIG_IR_SPI=y
CONFIG_LIRC=y
CONFIG_RC_CORE=y
CONFIG_RC_DECODERS=y
CONFIG_RC_DEVICES=y

# Mtd
CONFIG_MTD=y
CONFIG_MTD_BLOCK=y
CONFIG_MTD_BLOCK2MTD=y
CONFIG_MTD_OOPS=y

# Power
CONFIG_MI_WIRELESS=y
CONFIG_BQ_FUEL_GAUGE=y

# Regulators
CONFIG_LDO_WL2866D=y
CONFIG_REGULATOR_FAN53555=y
CONFIG_REGULATOR_WL2866D=y

# SPSS
# CONFIG_QCOM_SPSS is not set
# CONFIG_RPMSG_QCOM_GLINK_SPSS is not set

# Thermal
CONFIG_MI_THERMAL_INTERFACE=y

# Time-of-flight sensor
CONFIG_STMVL53L5=y

# Touchscreen
CONFIG_TOUCHSCREEN_ST_FTS_V521_SPI=y
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM_S3907=y
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM_SPI=y
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM_CORE=y
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM_REFLASH=y
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=y

# USB
CONFIG_USB_NET_AX88179_178A=y
CONFIG_USB_NET_AX8817X=y

# Drivers
CONFIG_EXTEND_RECLAIM=y
CONFIG_BINDER_PRIO=y
# CONFIG_BINDER_PRIO_DEBUG is not set
CONFIG_XLOGCHAR=y
CONFIG_HEALTH_GOODIX=y
